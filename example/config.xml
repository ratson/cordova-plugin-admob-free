<?xml version='1.0' encoding='utf-8'?>
<widget id="hello.admob" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello AdMob</name>
    <description>
        A sample cordova-plugin-admob-free application.
    </description>
    <author email="ratson@users.noreply.github.com" href="https://github.com/ratson/cordova-plugin-admob-free">
        Ratson
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.2.1" />
    <engine name="ios" spec="~4.2.0" />
    <plugin name="cordova-plugin-admob-free" spec="~0.1.3" />
</widget>
